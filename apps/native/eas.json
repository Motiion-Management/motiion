{
  "build": {
    "monorepo": {
      "pnpm": "9.0.6",
      "env": {
        "EXPO_USE_FAST_RESOLVER": "true"
      },
      "channel": "monorepo"
    },
    "development": {
      "env": {
        "APP_VARIANT": "development"
      },
      "extends": "monorepo",
      "developmentClient": true,
      "distribution": "internal",
      "environment": "development",
      "channel": "development"
    },
    "dev-sim": {
      "extends": "development",
      "ios": {
        "simulator": true
      },
      "channel": "dev-sim"
    },
    "production": {
      "env": {
        "APP_VARIANT": "production"
      },
      "extends": "monorepo",
      "distribution": "store",
      "autoIncrement": true,
      "environment": "production",
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6748365434",
        "bundleIdentifier": "io.motiion.motiion"
      }
    }
  },
  "cli": {
    "appVersionSource": "remote"
  }
}
