{
  "expo": {
    "name": "Motiion",
    "description": "The place for dance talent and choreographers.",
    "slug": "motiion",
    "version": "1.0.0",
    "scheme": "motiion",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "sdkVersion": "UNVERSIONED",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1c1d20"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "io.motiion.motiion",
      "supportsTablet": true,
      "entitlements": {
        "com.apple.developer.associated-domains": ["applinks:motiion.io"],
        "com.apple.developer.authentication-services.autofill-credential-provider": true,
        "com.apple.developer.usernotifications.communication": true,
        "aps-environment": "production",
        "com.apple.developer.applesignin": ["Default"],
        "com.apple.developer.usernotifications.time-sensitive": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1c1d20"
      },
      "package": "io.motiion.motiion"
    },
    "web": {
      "favicon": "./assets/favicon.ico"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-localization",
      "expo-secure-store"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "26b62811-8f3e-4bab-9548-9ab85dfa7cb2"
      }
    },
    "owner": "motiion"
  }
}
