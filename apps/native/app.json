{
  "expo": {
    "name": "Motiion",
    "description": "The place for dance talent and choreographers.",
    "slug": "motiion",
    "version": "1.0.0",
    "scheme": "motiion",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1c1d20"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "io.motiion.motiion",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1c1d20"
      },
      "package": "io.motiion.motiion"
    },
    "web": {
      "favicon": "./assets/favicon.ico"
    },
    "plugins": ["expo-font", "expo-router"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "26b62811-8f3e-4bab-9548-9ab85dfa7cb2"
      }
    },
    "owner": "motiion"
  }
}
