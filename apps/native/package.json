{
  "name": "native-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "dev": "expo start --dev-client",
    "start": "expo start --dev-client",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "build:dev": "eas build --profile development",
    "build:dev:sim": "eas build --profile dev-sim --platform ios --local",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:preview": "eas build --profile preview",
    "build:preview:ios": "eas build --profile preview --platform ios",
    "build:prod": "eas build --profile production",
    "build:prod:ios": "eas build --profile production --platform ios",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "migrate-tokens": "node scripts/migrate-tokens-simple.js",
    "migrate-tokens:dry": "node scripts/migrate-tokens-simple.js --dry-run",
    "migrate-figma-tokens": "node scripts/migrate-to-figma-tokens.js",
    "migrate-figma-tokens:dry": "node scripts/migrate-to-figma-tokens.js --dry-run",
    "figma:connect": "figma connect create",
    "figma:publish": "figma connect publish",
    "figma:unpublish": "figma connect unpublish",
    "web": "expo start --web"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.11.2",
    "@expo/react-native-action-sheet": "^4.1.1",
    "@expo/ui": "~0.1.1-alpha.7",
    "@expo/vector-icons": "^14.1.0",
    "@gorhom/bottom-sheet": "^5.1.4",
    "@hookform/resolvers": "^3.4.2",
    "@packages/backend": "workspace:*",
    "@packages/web-ui": "workspace:*",
    "@react-native-community/datetimepicker": "8.3.0",
    "@react-native-community/slider": "4.5.6",
    "@react-native-picker/picker": "2.11.0",
    "@react-navigation/native": "^7.1.6",
    "@rn-primitives/accordion": "^1.1.0",
    "@rn-primitives/alert-dialog": "^1.1.0",
    "@rn-primitives/aspect-ratio": "^1.1.0",
    "@rn-primitives/avatar": "^1.1.0",
    "@rn-primitives/checkbox": "^1.1.0",
    "@rn-primitives/dropdown-menu": "^1.1.0",
    "@rn-primitives/hooks": "^1.2.0",
    "@rn-primitives/label": "^1.1.0",
    "@rn-primitives/portal": "^1.2.0",
    "@rn-primitives/progress": "^1.1.0",
    "@rn-primitives/radio-group": "^1.1.0",
    "@rn-primitives/select": "^1.1.0",
    "@rn-primitives/separator": "^1.1.0",
    "@rn-primitives/switch": "^1.1.0",
    "@rn-primitives/toggle-group": "^1.1.0",
    "@rn-primitives/tooltip": "^1.1.0",
    "@rn-primitives/types": "^1.1.0",
    "@roninoss/icons": "^0.0.4",
    "@shopify/flash-list": "1.7.6",
    "@tanstack/react-form": "^1.6.3",
    "@tanstack/react-query": "^5.74.4",
    "@tanstack/zod-form-adapter": "^0.42.1",
    "@xixixao/uploadstuff": "^0.0.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "convex": "^1.24.1",
    "date-fns": "^3.6.0",
    "expo": "53.0.8",
    "expo-camera": "~16.1.2",
    "expo-constants": "~17.1.6",
    "expo-crypto": "~14.1.4",
    "expo-dev-client": "~5.1.8",
    "expo-font": "~13.3.1",
    "expo-image": "~2.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.4",
    "expo-linking": "~7.1.4",
    "expo-navigation-bar": "~4.2.4",
    "expo-notifications": "~0.31.1",
    "expo-router": "5.0.6",
    "expo-secure-store": "~14.2.3",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-store-review": "~8.1.5",
    "expo-system-ui": "~5.0.7",
    "expo-video": "~2.1.8",
    "expo-video-thumbnails": "~9.1.3",
    "expo-web-browser": "~14.1.6",
    "input-otp": "^1.2.4",
    "nativewind": "latest",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.51.5",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-keyboard-controller": "^1.17.1",
    "react-native-otp-entry": "^1.8.4",
    "react-native-pager-view": "6.7.1",
    "react-native-phone-entry": "^0.2.2",
    "react-native-reanimated": "3.17.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-uitextview": "^2.0.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "tailwind-merge": "^3.2.0"
  },
  "dependenciesMeta": {
    "react": {
      "injected": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@figma/code-connect": "^1.3.3",
    "@types/react": "~19.0.14",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.1",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.8.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "enabled": false,
        "listUnknownPackages": false
      }
    },
    "install": {
      "exclude": [
        "react-native@~0.76.6",
        "react-native-reanimated@~3.16.1",
        "react-native-gesture-handler@~2.20.0",
        "react-native-screens@~4.4.0",
        "react-native-safe-area-context@~4.12.0",
        "react-native-webview@~13.12.5"
      ]
    }
  },
  "private": true
}
