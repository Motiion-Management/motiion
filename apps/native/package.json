    "dev": "expo start --dev-client",
    "start": "expo start --dev-client",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "build:dev": "eas build --profile development",
    "build:dev:sim": "eas build --profile dev-sim --platform ios",
