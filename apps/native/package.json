{
  "name": "native-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "dev": "APP_VARIANT=development expo start --dev-client --scheme=motiion-dev",
    "start": "expo start --dev-client",
    "ios": "APP_VARIANT=development expo run:ios",
    "android": "expo run:android",
    "build:dev": "eas build --profile development",
    "build:dev:sim": "eas build --profile dev-sim --platform ios",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:preview": "eas build --profile preview",
    "build:preview:ios": "eas build --profile preview --platform ios",
    "build:prod": "eas build --profile production",
    "build:prod:ios": "eas build --profile production --platform ios",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "type-check": "tsc --noEmit",
    "migrate-tokens": "node scripts/migrate-tokens-simple.js",
    "migrate-tokens:dry": "node scripts/migrate-tokens-simple.js --dry-run",
    "migrate-figma-tokens": "node scripts/migrate-to-figma-tokens.js",
    "migrate-figma-tokens:dry": "node scripts/migrate-to-figma-tokens.js --dry-run",
    "figma:connect": "figma connect create",
    "figma:publish": "figma connect publish",
    "figma:unpublish": "figma connect unpublish",
    "web": "expo start --web"
  },
  "dependencies": {
